    "                                                      # Program to print full pyramid \n",
    "rows = int(input(\"Enter the number of rows\"));\n",
    "for i in range(0, rows):\n",
    "\tfor j in range(0, rows-i-1):\n",
    "\t\tprint(end=\" \")\n",
    "\tfor j in range(0, i+1):\n",
    "\t\tprint(\"*\", end=\" \")\n",
    "\tprint()"
